<launch>


        <node pkg="wifi_ddwrt" name="wifi_ddwrt" type="ddwrt" output="log">

            <param name="aps_file" value="$(find wifi_ddwrt)/config/ddwrt.yaml"/>

        </node>

        <node pkg="wifi_ddwrt" name="fingerprinting" type="fingerprinting" output="log">

            <rosparam file="$(find wifi_ddwrt)/config/ddwrt.yaml" command="load"/>

        </node>

</launch>

